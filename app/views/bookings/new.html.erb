<div class="form-wrapper" style="width: 400px; margin: 0 auto;">
  <%= render 'bookings/booking-form' %>
</div>

    <div id='flat-booking-dates' data-unavailable="<%= @item.unavailable_dates.to_json %>">
      <%= simple_form_for [ @item, @booking ] do |f| %>
        <%= f.input :starting_date, as: :string, required: true, input_html: {class: "datepicker"} %>
        <%= f.input :end_date, as: :string, required: true, input_html: {class: "datepicker"} %>
        <%= f.button :submit, "Go", class: "btn btn-primary" %>
      <% end %>
    </div>
  </div>
</div>

