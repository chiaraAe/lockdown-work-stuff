<div class="navbar">
  <div>Logo</div>
  <div>
    <% if user_signed_in? %>
    <%= "#{current_user[:email]}" %>
    <% end %>
  </div>
  <div class="navbar-right">
    <% if user_signed_in? %>
      <%= link_to "Log out", destroy_user_session_path, :method=>'delete'%>
    <% else %>
      <%= link_to "Log in", new_user_session_path %>
    <% end %>
    <div class="avatar"></div>
  </div>
</div>
